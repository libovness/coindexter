<div class="row">
	<div class="col-md-6 col-md-offset-3 main">
		<ul>
			<li class="user-header user-info-block block">
				<div class="row">
					<div class="col-md-8 col-md-offset-4">
						<h1>Feed</h1>
					</div>
				</div>
			</li>
			<% @logs.each do |log| %>
				<% puts "the user is #{log[:user]}" %>
				<% if log[:feed_type] == "network_log" %>
					<%= render(partial: 'networks/log', locals: {log: log}) %>
					<div class="row">
						<div class="col-md-8 col-md-offset-4">
							<div class="user-log-classification">
								<p>Coin</p>
								<div class="user-log-classification-network">
									<%= image_tag(log[:networks].logo.small.url, size: "20x20") %>
									<%= link_to log[:networks].name, log[:networks] %>
								</div>
							</div>
						</div>
					</div>
				<% elsif log[:feed_type] == "coin_log" %>
					<%= render(partial: 'coins/log', locals: {log: log}) %>
					<div class="row">
						<div class="col-md-8 col-md-offset-4">
							<div class="user-log-classification">
								<p>Network</p>
								<div class="user-log-classification-coin">
									<%= image_tag(log[:coins].logo.small.url, size: "20x20") %>
									<%= link_to log[:coins].name, log[:coins] %>
								</div>
							</div>
						</div>
					</div>
				<% elsif log[:feed_type] == "link" %>
					<%= render(partial: 'links/show_all_links', locals: {link: log[:data]}) %>
				<% end %>
			<% end %>
		</ul>
	</div>
</div>
