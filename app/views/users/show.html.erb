<div class="row">
	<div class="col-md-2 col-md-offset-1 left-nav">
		<%= render(partial: 'info_menu', object: {user: @user}) %>
	</div>
	<div class="col-md-6 main">
		<ul>
			<li class="user-header user-info-block block">
				<%= render(partial: 'user_main_info_header') %>
			</li>
			<% @logs.each do |log| %>
				<% if log.feed_type == "network_log" %>
					<%= render(partial: 'networks/log', locals: {log: log}) %>
				<% elsif log.feed_type == "coin_log" %>
					<%= render(partial: 'coins/log', locals: {log: log}) %>
				<% elsif log.feed_type == "link" %>
					<%= render(partial: 'links/show_all_links', locals: {link: log.data, coins: log.coins, networks: log.networks}) %>
				<% end %>
			<% end %>
		</ul>
	</div>
</div>
