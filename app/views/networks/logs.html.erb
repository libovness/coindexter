<div class="row">
	<div class="col-md-2 col-md-offset-1 left-nav">
		<%= render(partial: '/layouts/info_menu', locals: { network: @network, active_item: "log"}) %>
	</div>
	<%= render partial: '/networks/related_dropdown', locals: { network: @network } %>
	<div class="col-md-9 col-xs-12 main">
		<div class="info-header">
			<%= render(partial: 'networks/network_main_info_header', locals: { network: @network }) %>
		</div>
		<div class="content">
			<div class="row">
				<div class="col-md-8 col-xs-12 logs-cnt">
					<div class="row">
						<ul>
							<%= render(partial: 'log', collection: @logs, as: :log, locals: {show_tag: false}) %>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>