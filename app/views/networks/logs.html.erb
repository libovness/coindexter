<div class="row">
	<div class="col-md-2 col-md-offset-1 left-nav">
		<%= render(partial: '/layouts/info_menu', locals: { network: @network, active_item: "logs"}) %>
	</div>
	<div class="col-md-9 main">
		<div class="info-header">
			<%= render(partial: 'networks/network_main_info_header', locals: { network: @network }) %>
		</div>
		<div class="content">
			<div class="row">
				<div class="col-md-8">
					<div class="row">
						<ul>
							<%= render(partial: 'log', collection: @logs, as: :log) %>
						</ul>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>