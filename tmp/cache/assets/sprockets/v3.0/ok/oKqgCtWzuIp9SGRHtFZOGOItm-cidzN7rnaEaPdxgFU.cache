I"ñ(function() {
  $(function() {
    var $img, $switchState, $text, chart;
    $text = $('.account').text();
    $img = $('.account').find('img');
    $('.has-application').bootstrapSwitch('state', true);
    $switchState = $('.has-application').bootstrapSwitch('state');
    $('.application-info').prop('disabled', !$switchState);
    $('.account').mouseover(function() {
      $(this).text(' Signout');
      return $(this).prepend($img);
    });
    $('.account').mouseout(function() {
      $(this).text($text);
      return $(this).prepend($img);
    });
    $('.bootstrap-switch-id-coin_has_application').on('switchChange.bootstrapSwitch', function() {
      return $('.application-info').prop('disabled', $switchState);
    });
    $('ul#info-menu > li').click(function() {
      var selector;
      selector = 'ul#' + $(this).attr('id');
      $('.show-all').removeClass('active-item');
      $('ul.full-category-cnt').hide();
      return $(selector).show();
    });
    $('#show-all').click(function() {
      return $('ul.full-category-cnt').show();
    });
    $("#add-another-repo").on("click", function() {
      return $(this).parent().parent().after('<div class="row"><div class="col-md-4 label"><label class="form-label select-label" for="coin_Repo name">Repo name</label></div><div class="col-md-8"><input type="text" name="repositories" id="repositories" value="steemit"></div></div><div class="row"><div class="col-md-4 label"><label class="form-label select-label" for="coin_Repo url">Repo url</label></div><div class="col-md-8"><input type="text" name="repositories" id="repositories" value="https://github.com/steemit/steemit.com"><a class="add-another" id="add-another-repo">Add another repo</a></div></div>');
    });
    if ($(".coin-main-info").length > 0) {
      chart = new cryptowatch.Embed('coinbase', 'btcusd', {
        height: 300,
        customColorScheme: {
          bg: 'fdeee9',
          text: '545454',
          textStrong: '545454',
          textWeak: '999999',
          short: 'EB3333',
          shortFill: 'EB3333',
          long: '23a06d',
          longFill: '23a06d',
          cta: '000000',
          ctaHighlight: '000000',
          alert: '000000'
        }
      });
      return chart.mount('#chart');
    }
  });

}).call(this);
